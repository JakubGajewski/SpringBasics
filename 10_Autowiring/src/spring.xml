<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
    <bean id="triangle" class="pl.jakubgajewski.Triangle" name="Wojciech" autowire="byName" scope="prototype">
        <property name="type" value="EQUILATERAL" />
    </bean>
    <bean id="pointA" class="pl.jakubgajewski.Point">
        <property name="x" value="0"/>
        <property name="y" value="0"/>
    </bean>
    <bean id="pointB" class="pl.jakubgajewski.Point">
        <constructor-arg value="100"></constructor-arg>
        <constructor-arg value="100"/>
    </bean>
    <bean id="pointC" class="pl.jakubgajewski.Point">
        <constructor-arg value="30"/>
        <constructor-arg value="30"/>
    </bean>
    <alias name="Wojciech" alias="Wojtek"></alias>
</beans>